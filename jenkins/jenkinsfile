pipeline{
    agent any
    stages{
        stage('BUILD'){
             steps{
               sh '''
                 #!/bin/bash
                 pwd
                 ls
                 echo "this is BUILD stage"
                 sleep 5
               '''
             }
        }
         stage('DEPLOY'){
            steps{
                echo "this is DEPLOY stage"
                sh 'sleep 5'
             }
        }
          stage(TESTING){
         stage('TESTING1'){
            steps{
               echo "echo this is TESTING1 stage"
               sh 'sleep 5'
             }
        }
         stage('TESTING2'){
            steps{
                sh '''
                echo "this is TESTING2 stage"
                sleep 5
                '''
             }
        }
          }
    }
}