pipeline{
    agent any
    stages{
        stage('BUILD'){
             steps{
               sh '''
                 #!/bin/bash
                 pwd
                 ls
                 echo "this is BUILD stage"
               '''
             }
        }
         stage('DEPLOY'){
            steps{
                echo "this is DEPLOY stage"
             }
        }
         stage('TESTING1'){
            steps{
               echo "echo this is TESTING1 stage"
             }
        }
         stage('TESTING2'){
            steps{
                sh '''
                echo "this is TESTING2 stage"
                '''
             }
        }
    }
}